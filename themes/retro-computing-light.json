  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#8c8273",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#885020"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "scope": ["constant.other", "constant.character"],
      "settings": {
        "foreground": "#8a6020"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#a05000"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#a05000"
      }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#6a645a"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type"],
      "settings": {
        "foreground": "#805010"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#604020"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#3a352c"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#5a5448"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#8a6020"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#a05000"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#885020"
      }
    },
    {
      "scope": ["support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#805010"
      }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#a05000",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#885020"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#8c8273",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#a05000"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#406080"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#5a7030"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#884040"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#996600"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#884040"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#996600"
      }
    }
  ]
}
