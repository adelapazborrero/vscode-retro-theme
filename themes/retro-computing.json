  "tokenColors": [
    {
      "scope": ["comment", "punctuation.definition.comment"],
      "settings": {
        "foreground": "#6a645a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["string", "string.quoted"],
      "settings": {
        "foreground": "#b89060"
      }
    },
    {
      "scope": ["constant.numeric", "constant.language"],
      "settings": {
        "foreground": "#d4a050"
      }
    },
    {
      "scope": ["constant.other", "constant.character"],
      "settings": {
        "foreground": "#c8a060"
      }
    },
    {
      "scope": ["keyword", "storage.type", "storage.modifier"],
      "settings": {
        "foreground": "#e0a030"
      }
    },
    {
      "scope": ["keyword.control"],
      "settings": {
        "foreground": "#e0a030"
      }
    },
    {
      "scope": ["keyword.operator", "punctuation"],
      "settings": {
        "foreground": "#a09080"
      }
    },
    {
      "scope": ["entity.name.type", "entity.name.class", "support.type"],
      "settings": {
        "foreground": "#c0a070"
      }
    },
    {
      "scope": ["entity.name.function", "support.function", "meta.function-call"],
      "settings": {
        "foreground": "#d4b888"
      }
    },
    {
      "scope": ["variable", "variable.other"],
      "settings": {
        "foreground": "#d4c8b4"
      }
    },
    {
      "scope": ["variable.parameter"],
      "settings": {
        "foreground": "#b8a888"
      }
    },
    {
      "scope": ["variable.other.constant"],
      "settings": {
        "foreground": "#c8a060"
      }
    },
    {
      "scope": ["entity.name.tag"],
      "settings": {
        "foreground": "#e0a030"
      }
    },
    {
      "scope": ["entity.other.attribute-name"],
      "settings": {
        "foreground": "#b89060"
      }
    },
    {
      "scope": ["support.class", "entity.other.inherited-class"],
      "settings": {
        "foreground": "#c0a070"
      }
    },
    {
      "scope": ["meta.preprocessor", "entity.name.function.preprocessor"],
      "settings": {
        "foreground": "#cc9900"
      }
    },
    {
      "scope": ["markup.heading"],
      "settings": {
        "foreground": "#ffc333",
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.bold"],
      "settings": {
        "fontStyle": "bold"
      }
    },
    {
      "scope": ["markup.italic"],
      "settings": {
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.inline.raw", "markup.fenced_code"],
      "settings": {
        "foreground": "#b89060"
      }
    },
    {
      "scope": ["markup.quote"],
      "settings": {
        "foreground": "#6a645a",
        "fontStyle": "italic"
      }
    },
    {
      "scope": ["markup.list"],
      "settings": {
        "foreground": "#e0a030"
      }
    },
    {
      "scope": ["markup.underline.link"],
      "settings": {
        "foreground": "#7090a0"
      }
    },
    {
      "scope": ["meta.diff.header"],
      "settings": {
        "foreground": "#ffc333"
      }
    },
    {
      "scope": ["markup.inserted"],
      "settings": {
        "foreground": "#8a9a50"
      }
    },
    {
      "scope": ["markup.deleted"],
      "settings": {
        "foreground": "#aa5555"
      }
    },
    {
      "scope": ["markup.changed"],
      "settings": {
        "foreground": "#cc9900"
      }
    },
    {
      "scope": ["invalid"],
      "settings": {
        "foreground": "#cc5555"
      }
    },
    {
      "scope": ["invalid.deprecated"],
      "settings": {
        "foreground": "#cc9900"
      }
    },
    {
      "scope": ["entity.name.namespace", "entity.name.module"],
      "settings": {
        "foreground": "#c0a070"
      }
    },
    {
      "scope": ["support.other.variable"],
      "settings": {
        "foreground": "#d4c8b4"
      }
    },
    {
      "scope": ["string.regexp"],
      "settings": {
        "foreground": "#c09060"
      }
    },
    {
      "scope": ["constant.character.escape"],
      "settings": {
        "foreground": "#cc9900"
      }
    },
    {
      "scope": ["meta.object-literal.key"],
      "settings": {
        "foreground": "#d4b888"
      }
    },
    {
      "scope": ["entity.name.section"],
      "settings": {
        "foreground": "#ffc333"
      }
    }
